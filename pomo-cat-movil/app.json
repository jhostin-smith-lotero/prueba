{
  "expo": {
    "name": "pomo-cat-movil",
    "slug": "pomo-cat-movil",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/coin.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/images/coin.png"
    },
    "plugins": [
      "expo-web-browser",
      "expo-font",
      "expo-router",
      "expo-secure-store"
    ]
  }
}
